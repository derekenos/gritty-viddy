{
  "name": "gritty-viddy",
  "version": "1.0.0",
  "description": "Realish-time in-browser video effects using vanilla Javascript.",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/derekenos/gritty-viddy.git"
  },
  "browserslist": [
    "chrome >= 78",
    "firefox >= 71",
    "safari >= 13",
    "ios >= 13.2",
    "edge >= 18"
  ],
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "useBuiltIns": "usage",
          "corejs": "3.6.4"
        }
      ]
    ]
  },
  "scripts": {
    "build:js": "babel components/GrittyViddy.js -d build",
    "build:webpack": "npx webpack --mode=development",
    "build": "npm run build:js && npm run build:webpack",
    "dev": "serve"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.9.0",
    "html-webpack-inline-source-plugin": "0.0.10",
    "install": "^0.13.0",
    "npm": "^6.14.6",
    "serve": "^11.3.2",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "core-js": "^3.6.4",
    "html-webpack-plugin": "^3.2.0"
  }
}
